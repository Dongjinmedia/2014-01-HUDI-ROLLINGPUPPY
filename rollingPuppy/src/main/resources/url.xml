<mappings>
	<mapping>
		<url>/index</url>
		<controller>com.puppy.controller.HomeController</controller>
	</mapping>

	<mapping>
		<url>/join</url>
		<controller>com.puppy.controller.member.JoinController</controller>
	</mapping>

	<mapping>
		<url>/login</url>
		<controller>com.puppy.controller.member.LoginController</controller>
	</mapping>

	<mapping>
		<url>/logout</url>
		<controller>com.puppy.controller.member.LogoutController</controller>
	</mapping>

	<mapping>
		<url>/main</url>
		<controller>com.puppy.controller.MainController</controller>
	</mapping>

	<mapping>
		<url>/chat/*</url>
		<controller>com.puppy.controller.chat.ChatController</controller>
	</mapping>

	<mapping>
		<url>/chat/getList</url>
		<controller>com.puppy.controller.chat.ChatListController</controller>
	</mapping>

	<mapping>
		<url>/chat/initMessage</url>
		<controller>com.puppy.controller.chat.MessageController</controller>
	</mapping>

	<mapping>
		<url>/error</url>
		<controller>com.puppy.controller.ErrorController</controller>
	</mapping>

	<mapping>
		<url>/search</url>
		<controller>com.puppy.controller.SearchController</controller>
	</mapping>

	<mapping>
		<url>/unitTest</url>
		<controller>com.puppy.controller.TestController</controller>
	</mapping>

	<mapping>
		<url>/mobile</url>
		<controller>com.puppy.controller.mobile.MobileController</controller>
	</mapping>
	
	<mapping>
		<url>/mobile/chat</url>
		<controller>com.puppy.controller.mobile.MobileChatController</controller>
	</mapping>
	
	<mapping>
		<url>/chat/getMyChatInfo</url>
		<controller>com.puppy.controller.chat.ChatInfoController</controller>
	</mapping>
	
	<mapping>
		<url>/markerClicker/getCurrentNumOfParticipant</url>
		<controller>com.puppy.controller.chat.NumOfParticipantController</controller>
	</mapping>
</mappings>