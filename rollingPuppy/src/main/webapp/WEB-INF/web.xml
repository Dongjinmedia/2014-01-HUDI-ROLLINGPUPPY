
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<!-- Servlet Area -->	
	<servlet>
		<servlet-name>Join Request</servlet-name>
		<servlet-class>com.puppy.controller.JoinController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Login Request</servlet-name>
		<servlet-class>com.puppy.controller.LoginController</servlet-class>
	</servlet>
	
	
	<!-- Servlet-mapping Area -->
	<servlet-mapping>
		<servlet-name>Join Request</servlet-name>
		<url-pattern>/join</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Login Request</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	
	
</web-app>
